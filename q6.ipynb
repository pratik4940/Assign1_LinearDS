   "source": [
    "def kthSmallest(arr, n, k):\n",
    "\n",
    "    arr.sort()\n",
    " \n",
    "   \n",
    "    return arr[k-1]\n",
    " \n",
    "\n",
    "if __name__=='__main__':\n",
    "    arr = [12, 3, 5, 7, 19]\n",
    "    n = len(arr)\n",
    "    k = 2\n",
    "    print(\"K'th smallest element is\",\n",
    "          kthSmallest(arr, n, k))\n",
    "    "
